# App
ENV=development
DEBUG=false
PROJECT_NAME="Trendy Lyrics API"

# Embedding
TEXT_EMBED_DIM=256
ENABLE_OPENAI_EMBEDDINGS=false
OPENAI_API_KEY=
OPENAI_EMBED_MODEL=text-embedding-3-small

# Feature flags
ENABLE_TRANSCRIBE=true
ENABLE_AUDIO_EMOTION=false
ENABLE_PITCH_ANALYSIS=false

# Transcribe defaults
TRANSCRIBE_MODEL_SIZE=small
TRANSCRIBE_COMPUTE_TYPE=default

# Audio analysis defaults
PITCH_MIN_NOTE=C2
PITCH_MAX_NOTE=C7
EMOTION_MODEL_ID=superb/hubert-large-superb-er

# External integrations
YOUTUBE_API_KEY=YOUR_YOUTUBE_DATA_API_KEY
SPRING_CALLBACK_URL=https://your-spring-server.example.com/callback

# Optional proxies
# HTTP_PROXY=http://proxy.company.local:8080
# HTTPS_PROXY=http://proxy.company.local:8080
